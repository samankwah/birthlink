  "trainingSchedule": {
    "week1": {
      "dates": "2025-08-14 to 2025-08-18",
      "focus": "Basic Training - 25 registrars",
      "sessions": [
        {
          "day": "Monday",
          "topic": "Application Overview & PWA Installation",
          "duration": "2 hours",
          "participants": "First cohort (Kumasi district)"
        },
        {
          "day": "Tuesday", 
          "topic": "Authentication & Birth Registration Workflow",
          "duration": "3 hours",
          "participants": "First cohort (Kumasi district)"
        },
        {
          "day": "Wednesday",
          "topic": "Offline Functionality & Sync Management",
          "duration": "2 hours",
          "participants": "Second cohort (Tamale district)"
        },
        {
          "day": "Thursday",
          "topic": "Multi-language Features & Troubleshooting",
          "duration": "2 hours",
          "participants": "Third cohort (Ho district)"
        },
        {
          "day": "Friday",
          "topic": "Q&A Session & Feedback Collection",
          "duration": "1 hour",
          "participants": "All trained registrars"
        }
      ]
    },
    "week2": {
      "dates": "2025-08-21 to 2025-08-25",
      "focus": "Advanced Training & Remaining 25 registrars",
      "sessions": [
        {
          "day": "Monday-Tuesday",
          "topic": "Intensive 2-day training for remaining registrars",
          "duration": "4 hours/day",
          "participants": "Remaining 25 registrars"
        },
        {
          "day": "Wednesday-Thursday",
          "topic": "Hands-on Practice & Scenario Testing",
          "duration": "3 hours/day", 
          "participants": "All registrars"
        },
        {
          "day": "Friday",
          "topic": "Launch Preparation & Final Q&A",
          "duration": "2 hours",
          "participants": "All registrars + supervisors"
        }
      ]
    }
  },
  
